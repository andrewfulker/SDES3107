void setup(){
  size(500,500);
}

void draw(){
  background(23,100,10);
strokeWeight(5);
if ((keyPressed == true) && (key == '1')) {
  line(50,0,50,500);}

if ((keyPressed == true) && (key == '2')) {
  line(100,0,100,500);}

if ((keyPressed == true) && (key == '3')) {
  line(150,0,150,500);}

if ((keyPressed == true) && (key == '4')) {
  line(200,0,200,500);}
  
if ((keyPressed == true) && (key == '5')) {
  line(250,0,250,500);}

if ((keyPressed == true) && (key == '6')) {
  line(300,0,300,500);}

if ((keyPressed == true) && (key == '7')) {
  line(350,0,350,500);}

if ((keyPressed == true) && (key == '8')) {
  line(400,0,400,500);}

if ((keyPressed == true) && (key == '9')) {
  line(450,0,450,500);}
  

}
